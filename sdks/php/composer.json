{
    "name": "flapjackhq/flapjack-search-php",
    "version": "0.1.0-beta.1",
    "description": "A fully-featured and blazing-fast PHP API client to interact with Flapjack Search. Drop-in replacement for algoliasearch-client-php.",
    "keywords": ["flapjack", "search", "full-text-search", "algolia-compatible", "api", "client", "php"],
    "type": "library",
    "homepage": "https://github.com/flapjackhq/flapjack-search-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Flapjack Team",
            "homepage": "https://github.com/flapjackhq"
        }
    ],
    "require": {
        "php": ">=8.1 !=8.3.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/psr7": "^2.0",
        "psr/http-message": "^1.1 || ^2.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "psr/simple-cache": "^1.0 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.91.3",
        "phpunit/phpunit": "^10.0",
        "vlucas/phpdotenv": "^5.4",
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
        "psr-4": { "Flapjack\\FlapjackSearch\\" : "lib/" },
        "files": [
            "lib/Http/Psr7/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Flapjack\\FlapjackSearch\\Tests\\" : "tests/" }
    },
    "suggest": {
        "guzzlehttp/guzzle": "If you prefer to use Guzzle HTTP client instead of the Http Client implementation provided by the package"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
